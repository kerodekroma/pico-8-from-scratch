pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
 prefix="this is a: "
end

function _draw()
 cls()
 print("rectangles", 5, 5)
 print("==========", 5, 10)
 
 print(prefix.."outlined rectangle", 5, 15)
 -- rect(x1, y1, x2, y2, color)
 rect(5, 25, 10, 30, 8) -- red outline square

 print(prefix.."filled rectangle", 5, 35)
 -- rectfill(x1, y1, x2, y2, color)
 rectfill(5, 45, 10, 50, 8) -- red filled square

 print("circles", 5, 55)
 print("==========", 5, 60)
 
 print(prefix.."outlined circle", 5, 65)
 -- circ(x, y, radius, color)
 circ(10, 80, 5, 8) -- red outlined circle

 print(prefix.."filled circle", 5, 90)
 -- circfill(x, y, radius, color)
 circfill(10, 105, 5, 8) -- red circle
 
 print("base color's palette", 5, 115)
 for i=0,15 do 
  rectfill(i*5, 122, i*5, 125, i) 
 end
end
